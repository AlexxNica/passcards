{
  "manifest_version": 2,
  "name": "passcards",
  "description": "A Simple Password Manager",
  "version": "0.3",
  "homepage_url": "https://robertknight.github.io/passcards",
  "permissions": [
    "activeTab",
    "clipboardWrite",
    "storage",
    "https://*.dropbox.com/"
  ],
  "browser_action": {
    "default_icon": "data/icons/icon-32.png",
    "default_popup": "data/panel.html"
  },
  "commands" : {
	"_execute_browser_action" : {
	  "suggested_key" : {
	    "default" : "Alt+Shift+P"
	  }
	}
  },
  "icons": {
    "32": "data/icons/icon-32.png",
    "64": "data/icons/icon-64.png"
  },
  "web_accessible_resources": [
    "data/chrome_dropbox_oauth_receiver.html"
  ],
  "background": {
    "page": "data/background.html"
  },
  "key": ""
}
