{
	"rules" : {
		"class-name" : true,
		"curly" : true,
		"no-consecutive-blank-lines" : true,
		"no-debugger" : true,
		"no-duplicate-key" : true,
		"no-duplicate-variable" : true,
		"no-empty" : true,
		"no-eval" : true,
		"no-unreachable" : true,
		"no-unused-expression" : true,
		"no-unused-variable" : true,
		"no-use-before-declare" : true,
		"variable-name" : [true,
			"allow-leading-underscore"
		],
		"use-strict" : [true,
			"check-module"
		]
	}
}
